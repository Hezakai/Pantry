<div class="card">
    <div class="card-header">
        Inventory
    </div>
    <div class="card-body">
        <ul>
            {{#each user.invs}}
                <li>{{this.name}}: {{this.amount}} {{this.unit}}</li>
            {{/each}}
        </ul>
    </div>
        <div class="card-footer">
        <button id="addInvItem">Add Inventory Item</button>
    </div>
</div>
